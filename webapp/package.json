    "image-webpack-loader": "^6.0.0",
    "json-loader": "^0.5.4",
    "kind-of": "^6.0.3",
    "loader-utils": "^1.2.3",
    "minimist": "^0.2.1",
    "node-sass": "^4.13.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "uglify-js": "^2.8.29",
    "url-loader": "^0.6.2",
    "webpack": "^2.6.1"
  },
  "scripts": {
    "start-dev": "npm run start-server & npm run watch",
    "start-server": "mvn spring-boot:run -Dspring-boot.run.jvmArguments=\"-Dspring.config.additional-location=file://$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER,npm -Duser.timezone=UTC\"",
    "start-server-nofe": "mvn spring-boot:run -P '!frontend' -Dspring-boot.run.jvmArguments=\"-Dspring.config.additional-location=file://$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER,npm -Duser.timezone=UTC\"",
    "start-server-debug": "mvn spring-boot:run -Dspring-boot.run.jvmArguments=\"-Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=y,address=5005 -Dspring.config.additional-location=file://$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER,npm -Duser.timezone=UTC\"",
    "start-server-nofe-debug": "mvn spring-boot:run -P '!frontend' -Dspring-boot.run.jvmArguments=\"-Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=y,address=5005 -Dspring.config.additional-location=file://$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER,npm -Duser.timezone=UTC\"",
    "start-server-nofe-debugn": "mvn spring-boot:run -P '!frontend' -Dspring-boot.run.jvmArguments=\"-Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=5005 -Dspring.config.additional-location=file://$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER,npm -Duser.timezone=UTC\"",
    "watch": "webpack --config webpack.config.js --progress --colors --watch --env.inlineSourceMap",
    "watch-ict": "webpack --config webpack.config_ict.js --progress --colors --watch --env.inlineSourceMap",
    "build": "webpack --config webpack.config.js --progress --env.production",
    "build-ict": "webpack --config webpack.config_ict.js --progress --env.production",
    "mojito-tm-import": "java -jar ../cli/target/mojito-cli-*.jar tm-import -r Mojito -s src/main/resources/xliff/",
    "mojito-tm-export": "java -jar ../cli/target/mojito-cli-*.jar tm-export -r Mojito -t src/main/resources/xliff/",
    "mojito-push": "java -jar ../cli/target/mojito-cli-*.jar push -r Mojito -s src/main/resources/properties/ -ft PROPERTIES_NOBASENAME",
    "mojito-pull": "cd src/main/resources/properties/; java -jar ../../../../../cli/target/mojito-cli-*.jar pull -r Mojito -lm \"fr:fr-FR,be:be-BY,ko:ko-KR,ru:ru-RU\" -ft PROPERTIES_NOBASENAME",
    "mojito-drop-export": "java -jar ../cli/target/mojito-cli-*.jar drop-export -r Mojito",
    "mojito-drop-import": "java -jar ../cli/target/mojito-cli-*.jar drop-import -r Mojito",
    "create-demo-data": "java -jar ../cli/target/mojito-cli-*-exec.jar demo-create -n Demo",
    "preinstall": "if type npx >/dev/null 2>&1; then npx npm-force-resolutions; fi"
  },
  "resolutions": {
    "kind-of": "^6.0.3",
    "minimist": "^0.2.1"
  },
  "author": "Moji Webapp Team"
}
