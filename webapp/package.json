{
  "name": "mojito-webapp",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "description": "Mojito translation management system",
  "private": true,
  "dependencies": {
    "alt": "^0.18.6",
    "alt-container": "^1.1.1",
    "alt-mixins": "^1.0.0",
    "bootstrap-sass": "^3.4.1",
    "classnames": "^2.2.6",
    "cldr-data": "^36.0.0",
    "cldrjs": "^0.5.5",
    "keycode": "^2.2.1",
    "keymirror": "~0.1.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "properties": "^1.2.1",
    "react": "^16.14.0",
    "react-bootstrap": "^0.31.5",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.8.6",
    "react-ga": "^2.6.0",
    "react-icons": "^5.4.0",
    "react-intl": "^2.9.0",
    "react-router": "^3.2.6",
    "react-router-bootstrap": "^0.23.3",
    "react-sidebar": "^2.1.4",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "css-loader": "^3.6.0",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "file-loader": "^4.3.0",
    "globals": "^15.13.0",
    "html-webpack-plugin": "^5.6.2",
    "image-webpack-loader": "^6.0.0",
    "loader-utils": "^3.3.1",
    "sass": "^1.83.4",
    "sass-loader": "^12.6.0",
    "style-loader": "^1.3.0",
    "uglify-js": "^3.19.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "optionalDependencies": {
    "fsevents": "^1.2.13"
  },
  "scripts": {
    "start-dev": "npm run start-server-nofe & npm run watch",
    "start-server": "mvn spring-boot:run -Dspring-boot.run.jvmArguments=\"-Dspring.config.additional-location=optional:file://$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER,npm -Duser.timezone=UTC\"",
    "start-server-debug": "mvn spring-boot:run -Dspring-boot.run.jvmArguments=\"-Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=y,address=5005 -Dspring.config.additional-location=optional:file://$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER,npm -Duser.timezone=UTC\"",
    "start-server-debugn": "mvn spring-boot:run -Dspring-boot.run.jvmArguments=\"-Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=5005 -Dspring.config.additional-location=optional:file://$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER,npm -Duser.timezone=UTC\"",
    "start-server-nofe": "mvn spring-boot:run -P '!frontend' -Dspring-boot.run.jvmArguments=\"-Dspring.config.additional-location=optional:file://$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER,npm -Duser.timezone=UTC\"",
    "start-server-nofe-debug": "mvn spring-boot:run -P '!frontend' -Dspring-boot.run.jvmArguments=\"-Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=y,address=5005 -Dspring.config.additional-location=optional:file://$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER,npm -Duser.timezone=UTC\"",
    "start-server-nofe-debugn": "mvn spring-boot:run -P '!frontend' -Dspring-boot.run.jvmArguments=\"-Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=5005 -Dspring.config.additional-location=optional:file://$HOME/.l10n/config/webapp/ -Dspring.profiles.active=$USER,npm -Duser.timezone=UTC\"",
    "watch": "webpack --config webpack.config.js --progress --color --watch --env inlineSourceMap",
    "watch-ict": "webpack --config webpack.config_ict.js --progress --color --watch --env inlineSourceMap",
    "build": "npm run lint && webpack --config webpack.config.js --progress --env production",
    "build-ict": "npm run lint && webpack --config webpack.config_ict.js --progress --env production",
    "lint": "eslint src/main/resources/public",
    "mojito-tm-import": "java -jar ../cli/target/mojito-cli-*.jar tm-import -r Mojito -s src/main/resources/xliff/",
    "mojito-tm-export": "java -jar ../cli/target/mojito-cli-*.jar tm-export -r Mojito -t src/main/resources/xliff/",
    "mojito-push": "java -jar ../cli/target/mojito-cli-*.jar push -r Mojito -s src/main/resources/properties/ -ft PROPERTIES_NOBASENAME",
    "mojito-pull": "cd src/main/resources/properties/; java -jar ../../../../../cli/target/mojito-cli-*.jar pull -r Mojito -lm \"fr:fr-FR,be:be-BY,ko:ko-KR,ru:ru-RU\" -ft PROPERTIES_NOBASENAME",
    "mojito-drop-export": "java -jar ../cli/target/mojito-cli-*.jar drop-export -r Mojito",
    "mojito-drop-import": "java -jar ../cli/target/mojito-cli-*.jar drop-import -r Mojito",
    "create-demo-data": "java -jar ../cli/target/mojito-cli-*-exec.jar demo-create -n Demo",
    "preinstall": "node/node_modules/npm/bin/npx-cli.js npm-force-resolutions"
  },
  "resolutions": {
    "kind-of": "^6.0.3",
    "minimist": "^0.2.1",
    "ini": "^1.3.8",
    "node-fetch": "^2.6.1",
    "yargs-parser": "18.1.2"
  },
  "author": "Moji Webapp Team"
}
